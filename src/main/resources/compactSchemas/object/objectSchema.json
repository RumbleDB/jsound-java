{ 
	"targetType" : {
		"objects": "arrayOfObjects"
	},
	"myObject": {
		"stringField": "string",
		"integerField": "integer",
		"objectField?": "myObject=null"
	},
	"myObjectType": "myObject",
	"objectType" : {
		"myObject": "myObject",
		"!requiredObject": "myObject",
		"nullableObject?": "myObject",
		"objectWithDefault": "myObject={\"stringField\": \"hello\", \"integerField\": 1}",
		"!requiredObjectWithDefault": "myObject={\"stringField\": \"hello\", \"integerField\": 1, \"objectField\": {\"stringField\": \"world!\", \"integerField\": 2}}",
		"anotherObject": "myObjectType",
		"@uniqueObject": "myObject"
	},
	"arrayOfObjects": [
		"objectType"
	]
}
